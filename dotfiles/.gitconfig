[user]
        name = Radoslaw
        email = rkieltyka@dataplicity.com
[core]
        editor = vim
[credential]
        helper = cache --timeout=10000
[merge]
        conflictstyle = diff3
[color]
        branch = auto
        diff = auto
        status = auto
        ui = true
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = red
[color "diff"]
        frag = magenta bold
        meta = yellow
        new = green bold
        old = red bold
        whitespace = red reverse
[color "status"]
        added = green
        changed = red
        untracked = yellow
